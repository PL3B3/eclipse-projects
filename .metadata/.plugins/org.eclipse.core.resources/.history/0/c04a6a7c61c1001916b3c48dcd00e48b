package pickle.pudlet;

import java.util.Scanner;
import java.io.File;
import java.io.IOException;
import java.io.FileNotFoundException;

public class OgGradeMaster {
	public static void main(String args[]) {
		
	}
	
	public int[] organizeCSV(String fileName, int bucketNum) {
		Scanner csvScanner;
		
		try {
			csvScanner = new Scanner(new File(fileName));
		} catch (IOException x) {
			System.err.printf("IOException: %-s%n", x);
		} catch (FileNotFoundException x) {
			System.err.printf("SORRY ASSHOLE NO SUCH FILE");
		} finally {
			if (csvScanner == null) {
				csvScanner.close();
			}
		}
	}
} 
